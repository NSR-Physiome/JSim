<!-- ========================================		INPUT VALUES		============================================= -->
	<component 	name="input_values" 
				cmeta:id="input_values">
		<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
			<rdf:Description rdf:about="#input_values">
				<rdf:value>
				Component to set all input values to 1.0 or a prescribed value.
				</rdf:value>
			</rdf:Description>
		</rdf:RDF>
		
		<variable name="PRA" 	initial_value="0.00852183"		units="mmHg" private_interface="none" public_interface="out"/>
	</component>
	
<!-- VOLUME RECEPTORS INPUT CONNECTIONS -->
	<connection>
		<map_components component_1="volume_receptors" component_2="input_values"/>
		<map_variables variable_1="PRA" variable_2="PRA"/>
	</connection>
	<connection>
		<map_components component_1="volume_receptors" component_2="environment"/>
		<map_variables variable_1="time" variable_2="time"/>
	</connection>

