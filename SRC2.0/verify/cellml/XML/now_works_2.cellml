            </apply>
            <apply>
              <plus/>
              <cn cellml:units="dimensionless"> 1.0 </cn>
              <apply>
                <exp/>
                <apply>
                  <divide/>
                  <apply>
                    <minus/>
                    <ci> Em </ci>
                    <ci> alpha2_E50 </ci>
                  </apply> 
