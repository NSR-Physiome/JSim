          </apply>
        </apply>
      </apply>
      
      <apply id="k43_calculation">
        <eq/>
        <ci> k43 </ci>
        <apply>
          <divide/>
          <ci> Nai </ci>
          <apply>
            <plus/>
            <ci> K3ni </ci>
