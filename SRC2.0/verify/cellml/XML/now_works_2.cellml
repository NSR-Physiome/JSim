          <ci> A_to </ci>
        </apply>
        <apply>
          <minus/>
          <apply>
            <times/>
            <ci> alpha1 </ci>
            <ci> I3 </ci>
          </apply>
          <apply>
            <times/>
            <apply>
              <plus/>
              <ci> beta1 </ci>
              <ci> alpha2 </ci>
            </apply>
            <ci> A_to </ci>
