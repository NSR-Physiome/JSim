              <times/>
              <cn cellml:units="dimensionless"> 2.0 </cn>
              <ci> F </ci>
              <ci> Vrel </ci>
            </apply>
            <ci> tau_rel </ci>
          </apply>
          <ci> Carel </ci>
          <apply>
            <divide/>
            <cn cellml:units="dimensionless"> 1.0 </cn>
            <apply>
              <plus/>
              <cn cellml:units="dimensionless"> 1.0 </cn>
              <apply>
                <power/>
                <apply>
                  <divide/>
                  <ci> Km_Carel </ci>
