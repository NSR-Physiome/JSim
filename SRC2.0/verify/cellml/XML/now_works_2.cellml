            </apply>
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> Cai </ci>
                <ci> Nai </ci>
              </apply>
              <apply>
                <times/>
                <ci> Kci </ci>
                <ci> Kcni </ci>
              </apply>
            </apply>
          </apply>
          <apply>
            <times/>
            <apply>
              <divide/>
              <ci> Nai </ci>
