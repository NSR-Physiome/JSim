        <ci> i_Ca_tr </ci>
        <apply>
          <times/>
          <apply>
            <divide/>
            <apply>
              <times/>
              <cn cellml:units="dimensionless"> 2.0 </cn>
              <ci> F </ci>
              <ci> Vrel </ci>
            </apply>
            <ci> tau_tr </ci>
          </apply>
          <ci> Caup </ci>
        </apply>
      </apply>
    </math>
