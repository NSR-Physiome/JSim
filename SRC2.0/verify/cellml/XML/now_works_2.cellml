                    <ci> K3ni </ci>
                  </apply>
                </apply>
              </apply>
            </apply>
          </apply>
        </apply>
      </apply>
      
      <apply id="k34_calculation">
        <eq/>
        <ci> k34 </ci>
        <apply>
          <divide/>
          <ci> Nao </ci>
          <apply>
            <plus/>
            <ci> K3no </ci>
            <ci> Nao </ci>
