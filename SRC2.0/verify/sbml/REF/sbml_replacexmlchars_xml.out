    <listOfRules>
      <assignmentRule variable="L">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <ci> D </ci>
            <pi/>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule variable="Wallt">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <plus/>
            <apply>
              <times/>
              <ci> D </ci>
              <cn> -0.5 </cn>
            </apply>
            <apply>
              <power/>
              <apply>
                <plus/>
                <apply>
                  <power/>
                  <apply>
                    <times/>
                    <ci> D </ci>
                    <cn> 0.5 </cn>
                  </apply>
                  <cn> 2 </cn>
                </apply>
                <apply>
                  <times/>
                  <ci> VWallARef </ci>
                  <cn> 0.31830989 </cn>
                </apply>
              </apply>
              <cn> 0.5 </cn>
            </apply>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule variable="SigTot">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <apply>
              <divide/>
              <csymbol encoding="text" definitionURL="http://www.sbml.org/sbml/symbols/time"> TenTot </csymbol>
              <ci> Wallt </ci>
            </apply>
            <cn> 1000 </cn>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule variable="Eps">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <divide/>
            <ci> D </ci>
            <ci> DEpsRef </ci>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule variable="VWallARef">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <ci> WalltRef </ci>
            <apply>
              <plus/>
              <ci> DWalltRef </ci>
              <ci> WalltRef </ci>
            </apply>
            <pi/>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule variable="DEpsRef">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <ci> Dp100 </ci>
            <apply>
              <plus/>
              <apply>
                <divide/>
                <apply>
                  <ln/>
                  <apply>
                    <divide/>
                    <ci> TenEpsRef </ci>
                    <ci> Cp1 </ci>
                  </apply>
                </apply>
                <ci> Cp2 </ci>
              </apply>
              <cn> 1 </cn>
            </apply>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule variable="ActLev">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <divide/>
            <cn> 1 </cn>
            <apply>
              <plus/>
              <cn> 1 </cn>
              <apply>
                <exp/>
                <apply>
                  <plus/>
                  <apply>
                    <times/>
                    <ci> Cmyo </ci>
                    <csymbol encoding="text" definitionURL="http://www.sbml.org/sbml/symbols/time"> TenTot </csymbol>
                    <cn> -1 </cn>
                  </apply>
                  <ci> Ctone </ci>
                </apply>
              </apply>
            </apply>
          </apply>
        </math>
      </assignmentRule>
      <algebraicRule>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <minus/>
            <ci> TenPass </ci>
            <apply>
              <times/>
              <ci> Cp1 </ci>
              <apply>
                <exp/>
                <apply>
                  <times/>
                  <ci> Cp2 </ci>
                  <apply>
                    <minus/>
                    <apply>
                      <divide/>
                      <ci> D </ci>
                      <ci> Dp100 </ci>
                    </apply>
                    <cn> 1 </cn>
                  </apply>
                </apply>
              </apply>
            </apply>
          </apply>
        </math>
      </algebraicRule>
      <algebraicRule>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <minus/>
            <ci> TenMaxAct </ci>
            <apply>
              <times/>
              <ci> Ca1 </ci>
              <apply>
                <exp/>
                <apply>
                  <times/>
                  <apply>
                    <power/>
                    <apply>
                      <divide/>
                      <apply>
                        <minus/>
                        <apply>
                          <divide/>
                          <ci> D </ci>
                          <ci> Dp100 </ci>
                        </apply>
                        <ci> Ca2 </ci>
                      </apply>
                      <ci> Ca3 </ci>
                    </apply>
                    <cn> 2 </cn>
                  </apply>
                  <cn> -1 </cn>
                </apply>
              </apply>
            </apply>
          </apply>
        </math>
      </algebraicRule>
      <algebraicRule>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <minus/>
            <csymbol encoding="text" definitionURL="http://www.sbml.org/sbml/symbols/time"> TenTot </csymbol>
            <apply>
              <plus/>
              <ci> TenPass </ci>
              <apply>
                <times/>
                <ci> ActLev </ci>
                <ci> TenMaxAct </ci>
              </apply>
            </apply>
          </apply>
        </math>
      </algebraicRule>
      <assignmentRule variable="P">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <times/>
            <apply>
              <divide/>
              <csymbol encoding="text" definitionURL="http://www.sbml.org/sbml/symbols/time"> TenTot </csymbol>
              <ci> D </ci>
            </apply>
            <cn> 15001.234 </cn>
          </apply>
        </math>
      </assignmentRule>
    </listOfRules>
  </model>
