<?xml version="1.0" encoding="UTF-8"?>
<html xmlns="http://www.w3.org/1999/xhtml">
  <body>
    <p>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
          <eq/>
          <ci>t.min</ci>
          <cn>0</cn>
        </apply>
      </math>
    </p>
    <p>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
          <eq/>
          <ci>t.max</ci>
          <cn>10</cn>
        </apply>
      </math>
    </p>
    <p>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
          <eq/>
          <ci>t.delta</ci>
          <cn>.25</cn>
        </apply>
      </math>
    </p>
    <p>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
          <eq/>
          <ci>x.min</ci>
          <cn>0</cn>
        </apply>
      </math>
    </p>
    <p>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
          <eq/>
          <ci>x.max</ci>
          <cn>1</cn>
        </apply>
      </math>
    </p>
    <p>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
          <eq/>
          <ci>x.delta</ci>
          <cn>.1</cn>
        </apply>
      </math>
    </p>
    <p>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
          <eq/>
          <ci>u</ci>
          <apply>
            <plus/>
            <ci>x</ci>
            <ci>t</ci>
          </apply>
        </apply>
      </math>
    </p>
    <p>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <apply>
          <eq/>
          <ci>v</ci>
          <apply>
            <ci type="function">u</ci>
            <cn>1</cn>
            <cn>3</cn>
          </apply>
        </apply>
      </math>
    </p>
  </body>
</html>

