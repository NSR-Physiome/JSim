            <ci> c </ci>
          </apply>
          <apply>
            <plus/>
            <ci> L_1 </ci>
            <apply>
              <times/>
              <ci> c </ci>
              <apply>
                <plus/>
                <cn cellml:units="dimensionless"> 1.0 </cn>
